<h1><strong>Retro Bowl Unblocked</strong></h1>


<html>
 
  <head>
  </head>
 
  <body>
    <!-- Display the Retro Bowl logo -->
    <img src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcSvGWuYuW-_jhBRTx8MBteosozKh9YwJ45pqg&s" alt="Retro Bowl Logo" style="display:block; margin: 0 auto;">
    
    <!-- The styled button underneath the image -->
    <a id="button648d0526c35240359c0f87c0d70d65e2" style="display:inline-block;text-decoration:none;background:#0026ff;font-size:18px;font-family:arial;cursor:pointer;color:#ffffff;padding:15px 30px;border-color:#000000;border-style:solid;border-radius:3px;border-width:3px;margin:20px auto;display:block;text-align:center">
      Play Retro Bowl
    </a>

    <style>
      #button648d0526c35240359c0f87c0d70d65e2:hover {
        background-color: #0011ff !important;
        color: #ffffff !important;
      }
    </style>

    <script>
      var urlObj = new window.URL(window.location.href);
      var url = "https://ubggo.github.io/ub-games/retrobowl/";
      if (url) {
        var win;

        document.querySelector('a').onclick = function(e) {
          e.preventDefault(); // Prevent the default link behavior
          if (win) {
            win.focus();
          } else {
            win = window.open();
            win.document.body.style.margin = '0';
            win.document.body.style.height = '100vh';
            var iframe = win.document.createElement('iframe');
            iframe.style.border = 'none';
            iframe.style.width = '100%';
            iframe.style.height = '100%';
            iframe.style.margin = '0';
            iframe.src = url;
            win.document.body.appendChild(iframe);
          }
        };
      }
    </script>
  </body>
 
</html>




